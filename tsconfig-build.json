{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM","DOM.Iterable","ESNext"],
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true
  },
  "include": ["src","lib"]
}
